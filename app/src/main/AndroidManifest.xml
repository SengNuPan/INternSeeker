<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sengnupan.android_pj">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".searchCompany"
            android:label="Have 25 companies"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".their_stories"
            android:label="List of companies"
            android:parentActivityName=".searchCompany" />
        <activity
            android:name=".their_activites"
            android:label="Albums"
            android:parentActivityName=".searchCompany" />
        <activity android:name=".Files" />
        <activity android:name=".webview"></activity>
    </application>

</manifest>